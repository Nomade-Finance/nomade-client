{
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "error"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/index.html",
      "check": true
    }
  ]
}
